  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch | pino-pretty -c -t",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/prisma/schema"
  },
  "dependencies": {
    "@fastify/cors": "^8.0.0",
    "@fastify/swagger": "^7.4.1",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.3",
    "@nestjs/graphql": "^10.0.18",
    "@nestjs/mercurius": "^10.0.17",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/platform-fastify": "^9.0.3",
    "@nestjs/swagger": "^6.0.4",
    "@prisma/client": "^4.0.0",
    "@sentry/node": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.1",
    "exif": "^0.6.0",
    "fastify": "^4.2.1",
    "fastify-swagger": "^5.2.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "graphql-type-json": "^0.3.2",
    "mercurius": "^10.1.0",
    "nestjs-pino": "^2.6.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "node-fetch": "^3.2.8",
    "pino-http": "^8.1.1",
    "pino-pretty": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.3",
    "@types/cache-manager": "^4.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.5",
    "@types/node": "^18.0.4",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vegardit/prisma-generator-nestjs-dto": "^1.5.1",
    "cache-manager": "^4.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "prisma-nestjs-graphql": "^16.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
